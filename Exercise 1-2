package javaapplication1;
import java.util.*;
public class Javaapplication1 {

    public static void main(String[] args) {
      int a,b;
        Scanner obj=new Scanner(System.in);
        System.out.println("enter 2 numbers");

        a=obj.nextInt();
        b=obj.nextInt();
        int c;
        c=a<b?a:b;
        
          System.out.println(c+"is lesser");
        
    }
}
